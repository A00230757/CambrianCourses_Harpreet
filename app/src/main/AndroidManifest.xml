        <activity android:name=".AddProfessorActivity" />
        <activity android:name=".AddCoursesActivity" />
        <activity android:name=".AddDepartmentActivity" />
        <activity android:name=".MainInterface" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.CambrianCourses_Harpreet.NoActionBar" />
    </application>

</manifest>